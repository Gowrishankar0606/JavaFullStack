package one.index;

public class NormalMethod {

	public void display()
	{
		System.out.println("Method with no parameter");
	}
	
	public void display2(int a)
	{
		System.out.println("Method with parameter"+a);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		NormalMethod GS=new NormalMethod();
		
		GS.display();
		GS.display2(20);
	}

}


//write a program to add a 2 number using static method
//write a program to add 2 numbers using public method